<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="cl.bennu.plcbus.common.domain.PoweredDevice" table="dispositivos_encendidos" lazy="false" >
        <id name="id" column="dpec_id">
            <generator class="native"/>
        </id>
        <property name="deviceId" column="dpst_id"/>
        <property name="time" column="dpec_fecha"/>
    </class>

</hibernate-mapping>